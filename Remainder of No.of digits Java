import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int temp=n;
        int count=0;
        while(n!=0){
            n=n/10;
            count++;
        }
        n=temp;
        int r=n%count;
        System.out.println(r);
    }
}
